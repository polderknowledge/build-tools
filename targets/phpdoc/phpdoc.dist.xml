<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <parser>
        <default-package-name>Default</default-package-name>
        <encoding>utf-8</encoding>
        <target>build/phpdoc-cache</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility></visibility>
        <files>
            <directory>.</directory>
            <ignore>tests/*</ignore>
            <ignore>vendor/*</ignore>
        </files>
    </parser>
    <transformer>
        <target>build/phpdoc</target>
    </transformer>
    <logging>
        <level>error</level>
    </logging>
    <transformations>
        <template name="clean"/>
    </transformations>
    <translator>
        <locale>en</locale>
    </translator>
</phpdocumentor>